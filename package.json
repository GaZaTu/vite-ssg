{
  "name": "@gazatu/vite-ssg",
  "version": "0.2.0",
  "description": "https://github.com/antfu/vite-ssg but for everyone (including your mom)",
  "author": "Anthony Fu <anthonyfu117@hotmail.com> and GaZaTu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaZaTu/vite-ssg"
  },
  "bugs": "https://github.com/GaZaTu/vite-ssg/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "ssg",
    "ssr"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "require": "./dist/node.cjs",
      "import": "./dist/node.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "dist/node.d.ts"
      ]
    }
  },
  "bin": {
    "vite-ssg": "bin/vite-ssg.js"
  },
  "files": [
    "dist",
    "bin",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0-0 || ^4.0.0-0"
  },
  "dependencies": {
    "critters": "^0.0.16",
    "fs-extra": "^10.1.0",
    "html-minifier": "^4.0.0",
    "html5parser": "^2.0.2",
    "jsdom": "^20.0.0",
    "kolorist": "^1.5.1",
    "p-queue": "^6.6.2",
    "prettier": "^2.7.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/html-minifier": "^4.0.2",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.18.38",
    "@types/prettier": "^2.7.0",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "rollup": "^2.77.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vite": "^3.0.4",
    "vite-plugin-pwa": "^0.12.3"
  }
}
